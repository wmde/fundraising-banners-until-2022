$color-primary: #3366cc;
$color-primary-light: #6e9fff;
$color-secondary: #eaf3ff;
$color-warning: #a21d0c;

$color-white: #ffffff;
$color-black: #000000;
$color-black-light: #222222;
$color-gray: #a9a9a9;
$color-gray-medium: #e4e3e3;
$color-gray-light: #fbfbfb;
$color-gray-dark: #535353;

$banner-sans-serif: 'Open Sans', 'Arial', 'Helvetica', 'Verdana', sans-serif;
$banner-serif: 'Cardo', 'Georgia', 'Times', 'Times New Roman', serif;

/*
Always work 'mobile first':
	do not use @media ( max-width: $breakpoint-x )
	always use @media ( min-width: $breakpoint-x )
*/
$breakpoint_xs: 840px;
$breakpoint_s: 1024px;
$breakpoint_m: 1100px;
$breakpoint_l: 1300px;
$breakpoint_xl: 1580px;

$banner-easing: cubic-bezier( 0.63, 0.155, 0.38, 0.885 );
$progressbar-gradient: linear-gradient( 270deg, $color-primary-light 1.09%, $color-primary 100% );

$icon-bank-transfer: url( 'data:image/svg+xml;base64,' );
$icon-credit-card: url( 'data:image/svg+xml;base64,' );
$icon-direct-debit: url( 'data:image/svg+xml;base64,' );
$icon-paypal: url( 'data:image/svg+xml;base64,' );
